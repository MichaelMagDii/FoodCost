<Window x:Class="Food_Cost.TheForms.Setups"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Food_Cost.TheForms"
        mc:Ignorable="d" Title="setups" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
        Height="150" Width="400">
    <Grid>
        <UniformGrid Rows="1">
            <Button Width="125" Margin="5" Background="Gray" Foreground="Black" FontSize="14" FontWeight="Bold" Click="OutletSetup_Click" Content="Outlet Setup" />
            <Button Width="125" Margin="5" Background="Gray" Foreground="Black" FontSize="14" FontWeight="Bold" Click="Reports_Click" Content="Reports"/>
            <Button Width="125" Margin="5" Background="Gray" Foreground="Black" FontSize="14" FontWeight="Bold" Click="EndMonth_Click" Content="End Month"/>
        </UniformGrid>
    </Grid>
</Window>
